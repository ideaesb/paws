package org.noaaideacenter.paws;

import java.util.Comparator;

public class AssessmentNameComparator implements Comparator<Assessment> 
{

	public AssessmentNameComparator() {
		// TODO Auto-generated constructor stub
	}

	public int compare(Assessment a1, Assessment a2)
	{
		/*
		ProjectBean p1bean = (ProjectBean) p1;
		ProjectBean p2bean = (ProjectBean) p2;
		*/
		
		String p1name = a1.getName();
		String p2name = a2.getName();
		
		return p1name.compareToIgnoreCase(p2name);
	}
	
}
